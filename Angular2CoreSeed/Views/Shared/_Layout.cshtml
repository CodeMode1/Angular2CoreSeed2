<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Angular2CoreSeed</title>
    <base href="/" />
    <link rel="stylesheet" href="~/dist/vendor.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/dist/app.min.css" asp-append-version="true" />
    <!-- Polyfill for hammer touch emulator -->
    <script src="http://cdn.rawgit.com/hammerjs/touchemulator/master/touch-emulator.js"></script>
    <!-- Polyfill for Web Animations -->
    <script src="https://unpkg.com/web-animations-js@2.2.1"></script>
    @*<script>
    TouchEmulator();
    </script>*@
</head>
<body>
    @RenderBody()
    @RenderSection("scripts", required: false)
</body>
</html>
